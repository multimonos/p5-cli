{
    "name": "p5-cli",
    "version": "1.0.0",
    "description": "p5 cli for generating sketches from templates",
    "main": "lib/index.js",
    "exports": "lib/index.modern.js",
    "module": "lib/index.module.js",
    "unpkg": "lib/index.umd.js",
    "bin": {
        "p5": "./lib/index.js"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "rm -rf ./lib && babel src --out-dir lib",
        "watch": "rm -rf ./lib && babel --watch src --out-dir lib"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/multimonos/p5-cli.git"
    },
    "keywords": [
        "p5js",
        "parceljs",
        "cli",
        "sketch",
        "templates"
    ],
    "author": "craig hopgood",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/multimonos/p5-cli/issues"
    },
    "homepage": "https://github.com/multimonos/p5-cli#readme",
    "dependencies": {
        "app-root-path": "^3.0.0",
        "chalk": "^4.1.0",
        "commander": "^7.0.0",
        "copy-dir": "^1.3.0",
        "lodash": "^4.17.20",
        "log-utils": "^1.0.0",
        "mkdirp": "^1.0.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11"
    }
}
